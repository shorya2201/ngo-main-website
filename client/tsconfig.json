{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "jsx": "react-jsx",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      
    "lib": ["dom", "dom.iterable", "es6"],
    "allowJs": true,
    
    
    "noEmit": true,
    
    
    "resolveJsonModule": true,
    "isolatedModules": true,
   
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
    },
    "include": ["src/**/*","next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
   
  "exclude": ["node_modules"]
  }